<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Todas las pruebas" parallel="classes"
	thread-count="2">
	<test name="Pruebas con identificacion necesaria">
		<classes>
			<class name="privado.AutenticacionRequeridaChrome" />
			<class name="privado.AutenticacionRequeridaFirefox" />
		</classes>
	</test>
	<test name="Pruebas sin identificacion">
		<classes>
			<class name="publico.SinAutenticarChrome" />
			<class name="publico.SinAutenticarFirefox" />
		</classes>
	</test>
</suite>
